extends layout

block content
  .container
  h1= title
  a.btn.btn-primary(href='/addContact') Add Contact
  
  table.table
    thead
      tr
        th Name
        th Email
        th Phone
    tbody#contactList
      each contact in contacts
        tr
          td= contact.name
          td= contact.email
          td= contact.phone
block scripts
  script(src='js/contactIndex.js')

